{
  "name": "LCAdressbookSQLite",
  "productName": "LetzteChance.Org - Adressbook SQLite",
  "version": "1.0.0",
  "author": "David Honisch",
  "description": "LetzteChance.Org - Adressbook SQLite - Hybrid Application for Windows,Android and IOS",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^19.48.3",
    "electron-packager": "^13.0.1",
    "electron-prebuilt": "^1.4.13",
    "file-loader": "^3.0.1"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "package-mac": "electron-packager . LCAdressbookSQLite --overwrite --platform=darwin --arch=x64 --icon=./favicon.ico.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . LCAdressbookSQLite --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"lc2searchnav\"",
    "package-linux": "electron-packager . LCAdressbookSQLite --overwrite --asar=true --platform=linux --arch=x64 --icon=./32x32.png --prune=true --out=release-builds",
    "transformtest": "node nodejs/nodejre.js",
    "transform": "node nodejs/nodejre.js",
    "query": "node getmaterialimport.js",
    "server": "node nodejs/server.js",
    "getmaterial": "node nodejs/getmaterialimport.js",
    "count": "node nodejs/sumimport.js",
    "connect": "node nodejs/connect.js",
    "startclient": "start http://localhost:5000",
    "test": "test"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "lc2searchnav",
    "X-Plattform",
    "Search",
    "demo"
  ],
  "license": "MIT",
  "build": {
    "appId": "com.hapag.lloyd.hlsap.ui.charts",
    "copyright": "Copyright Â© 2019 ${David Honisch}",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "title": "lc2searchnav",
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "bootstrap": "^4.3.1",
    "cheerio": "^0.22.0",
    "jquery": "^3.4.0",
    "popper.js": "^1.14.7",
    "sql.js": "^0.4.0",
    "tether": "^1.4.0"
  },
  "standard": {
    "globals": [
      "$"
    ]
  }
}
